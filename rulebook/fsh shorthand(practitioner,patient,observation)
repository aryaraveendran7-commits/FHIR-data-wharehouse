Instance: ExamplePractitioner
InstanceOf: Practitioner
Usage: #example

* identifier.system = "http://hospital.smarthealth.org/practitioners"
* identifier.value = "PRAC123"
* name.family = "Sharma"
* name.given = "Anita"
* gender = #female
* qualification.code.text = "MBBS"

Instance: ExamplePatient
InstanceOf: Patient
Usage: #example

* identifier.system = "http://hospital.smarthealth.org/patients"
* identifier.value = "PAT456"
* name.family = "Kumar"
* name.given = "Ravi"
* gender = #male
* birthDate = "1990-06-15"

Instance: ExampleObservation
InstanceOf: Observation
Usage: #example

* status = #final
* category = http://terminology.hl7.org/CodeSystem/observation-category#vital-signs
* code.text = "Blood Pressure"
* subject = Reference(ExamplePatient)
* performer = Reference(ExamplePractitioner)
* effectiveDateTime = "2025-01-10T10:30:00+05:30"

* component[0].code.text = "Systolic Blood Pressure"
* component[0].valueQuantity.value = 120
* component[0].valueQuantity.unit = "mmHg"

* component[1].code.text = "Diastolic Blood Pressure"
* component[1].valueQuantity.value = 80
* component[1].valueQuantity.unit = "mmHg"
